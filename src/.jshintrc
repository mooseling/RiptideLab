{
  "browser":true,
  "undef":true,
  "unused":true,
  "esversion":11,
  "globals":{"RiptideLab":true},

  // Unreachable statement after return
  // Doesn't account for hoisted async functions, which I use
  "-W027":true,

  // Misleading line breaks
  // Flags ?: statements split across lines
  "-W014":true,

  // Unorthodox function invocation
  // Flags path?.to?.function()
  "-W067":true
}
